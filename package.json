  "bugs": {
    "url": "https://github.com/itsbaska/better-tutor/issues"
  },
  "homepage": "https://github.com/itsbaska/better-tutor#readme",
  "dependencies": {
    "camelcase": "^5.0.0",
    "connect-wwwhisper": "^0.1.21",
    "decamelize": "^2.0.0",
    "dotenv": "^6.2.0",
    "email-templates": "^5.0.3",
    "express": "^4.16.4",
    "google-spreadsheet": "^2.0.7",
    "if-env": "^1.0.4",
    "jsonfile": "^5.0.0",
    "lodash": "^4.17.11",
    "moment": "^2.29.2",
    "node-cron": "^2.0.3",
    "nodemailer": "^5.1.1",
    "pug": "^2.0.3",
    "uniqid": "^5.0.3"
  },
